                                            @if (ViewBag.JavaScriptFunction != null)
                                            {
                                                <script type="text/javascript">
                                                @Html.Raw(ViewBag.JavaScriptFunction)
                                                </script>
                                            }
                                            <div id="container"></div>
                                        </div>
                                        @*<div id="textChoice" style="display:none">
                                                @Html.Raw(Model.Text)
